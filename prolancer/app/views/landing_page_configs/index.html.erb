
<p id="notice"><%= notice %></p>
<div class="col-lg-8 col-lg-offset-2 col-md-8 col-sm-8 col-md-offset-2 col-sm-offset-2">
<h1>Listing Landing Page Configs</h1>

<table>
  <thead>
    <tr>
      <th>Header</th>
      <th>Description</th>
      <th>Fa icon</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @landing_page_configs.each do |landing_page_config| %>
      <tr>
        <td><%= landing_page_config.header %></td>
        <td><%= landing_page_config.description %></td>
        <td><%= landing_page_config.fa_icon %></td>
        <td><%= link_to 'Edit', edit_landing_page_config_path(landing_page_config) %></td>
        <td><%= link_to 'Destroy', landing_page_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

<%= link_to 'New Landing page config', new_landing_page_config_path %>
